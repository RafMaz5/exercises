{
  "name": "myproject",
  "version": "0.0.1",
  "main": "index.html",
  "sap.ui5": {
	"_version": "1.1.0",
	"rootView": "sap.ui.iride.view.App",
	"dependencies": {
	  "minUI5Version": "1.30",
	  "libs": {
		"sap.m": {}
	  }
	},
  "window": {
  	"frame" : true,
  	"toolbar" : false,
  	"width" : 1024,
  	"height" : 768,
  	"min_width" : 320,
  	"min_height" : 640
  },
	"models": {
	  "i18n": {
		"type": "sap.ui.model.resource.ResourceModel",
		"settings": {
		  "bundleName": "sap.ui.iride.i18n.i18n"
		}
	  }
	},
	"resources": {
	  "css": [
		{
		  "uri": "css/style.css"
		}
	  ]
	},
	"routing": {
	  "config": {
		"routerClass": "sap.m.routing.Router",
		"viewType": "XML",
		"viewPath": "sap.ui.iride.view",
		"controlId": "app",
		"controlAggregation": "pages"
	  },
	  "routes": [
		{
		  "pattern": "",
		  "name": "overview",
		  "target": "overview"
		}
	  ],
	  "targets": {
		"overview": {
		  "viewName": "Overview"
		}
	  }
	}
  }
}
